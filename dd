
Print Fibonacci series
read n
a=0
b=1

echo "The fibonacci Series is "

for ((i=0;i<n;i++))
do
	echo -ne "$a\t"
	c=$((a + b))
	a=$b
	b=$c
done


#!/bin/bash

# Check if user provided a directory path
if [ -z "$1" ]; then
    echo "Usage: $0 <directory>"
    exit 1
fi

# List all files recursively in the provided directory
find "$1" -type f
